<br>
<div>
 &nbsp <%= link_to outlines_path, class: 'btn btn-western' do %> <span class="fa fa-thumbs-up"></span> VIEW OUTLINES <% end %>
</div>
<% if can? :create, Outline %>
  <div>
    <%= render 'shared/new_outline_modal', courses: @courses, templates: @templates %>
  </div>

<% end %>

<% if @user.guest? %>
  <%= render 'outlines/partials/outlines_view' %>
<% end %>

<% if @user.instructor? %>
  <%= render 'outlines/partials/outlines_create_view' %>
<% end %>

<% if @user.admin? %>
  <%= render 'outlines/partials/templates_create_view' %>
<% end %>

<%= form_for Outline.new, url: new_outline_path, method: :get do |f| %>
  <%= fields_for :course do |ff| %>
    <div>
      <%#= ff.autocomplete_field :course_code, autocomplete_course_code_courses_path, class: 'form-control' %>
    </div>
  <% end %>
<% end %>